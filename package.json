    "marked": "^17.0.1",
    "moment": "^2.30.1",
    "mongolass": "^4.4.7",
    "objectid-to-timestamp": "^1.3.0",
    "sha1": "^1.1.1",
    "winston": "^3.18.3"
  },
  "devDependencies": {
    "eslint": "^9.39.1",
